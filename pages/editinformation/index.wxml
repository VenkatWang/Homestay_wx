<van-notify id="van-notify" />
<van-cell-group border="{{false}}">
  <van-field value="{{user.uid}}" label="用户ID" disabled />
  <van-field value="{{user.uname}}" label="用户名" placeholder="请输入" bind:change="handleChange" data-name="uname"/>
  <van-field value="{{user.uage}}" label="年龄" placeholder="请输入" bind:change="handleChange" data-name="uage"/>
  <van-field value="{{user.usex}}" label="性别" placeholder="请输入" bind:change="handleChange" data-name="usex" />
  <van-field value="{{user.uphone}}" label="手机号码" disabled />
  <van-field value="{{user.create_time}}" label="注册时间" disabled />
  <van-field value="{{user.update_time}}" label="更新时间" disabled />
  <van-field value="" label="上传头像" border="{{false}}" disabled />
  <van-uploader deletable="{{ true }}" file-list="{{ fileList }}" bind:after-read="afterRead" max-count="1"/>
  <van-button type="primary" class="submitBtn">提交</van-button>
</van-cell-group>